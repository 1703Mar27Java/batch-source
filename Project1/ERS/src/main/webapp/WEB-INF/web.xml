<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
    
	<display-name>ERS</display-name>
	<error-page>
    <error-code>404</error-code>
    <location>/404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/500.jsp</location>
  </error-page>
	
	
	
	<servlet>
		<servlet-name>MasterServlet</servlet-name>
		<servlet-class>com.revature.servlets.MasterServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>com.revature.servlets.HomeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>com.revature.servlets.UserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PasswordServlet</servlet-name>
		<servlet-class>com.revature.servlets.PasswordServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.revature.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<servlet-class>com.revature.servlets.RegisterServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RequestServlet</servlet-name>
		<servlet-class>com.revature.servlets.RequestServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReimbursementServlet</servlet-name>
		<servlet-class>com.revature.servlets.ReimbursementServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ActionServlet</display-name>
		<servlet-name>ActionServlet</servlet-name>
		<servlet-class>com.revature.servlets.ActionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MasterServlet</servlet-name>
		<url-pattern>/app</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PasswordServlet</servlet-name>
		<url-pattern>/password</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/addUser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RequestServlet</servlet-name>
		<url-pattern>/request</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReimbursementServlet</servlet-name>
		<url-pattern>/reimbursement</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ActionServlet</servlet-name>
		<url-pattern>/action</url-pattern>
	</servlet-mapping>


	

</web-app>
