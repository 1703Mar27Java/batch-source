  <display-name>AjaxDemo</display-name>
  <servlet>
    <servlet-name>counter</servlet-name>
    <servlet-class>com.revature.web.CounterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>counter</servlet-name>
    <url-pattern>/count</url-pattern>
